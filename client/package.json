{
  "name": "www.huaraz-adventures.com",
  "version": "0.1.0",
  "description": "Huaraz Adventures",
  "main": "webpack.config.js",
  "scripts": {
    "start": "NODE_ENV=local webpack-dev-server --progress --inline",
    "build:dev": "NODE_ENV=development webpack --output-path build-dev",
    "push:dev": "aws s3 sync build-dev s3://www-dev.huaraz-adventures.com/ --delete --profile personal --region us-east-1",
    "invalidate:dev": "aws cloudfront create-invalidation --profile default --distribution-id=ENZYSI71EH3I --paths /index.html",
    "deploy:dev": "yarn build:dev && yarn push:dev && yarn invalidate:dev",
    "build:stats": "NODE_ENV=production webpack -p --output-path build-prod --json > stats.json",
    "build:prod": "NODE_ENV=production webpack -p --progress --output-path build-prod",
    "push:prod": "aws s3 sync build-prod s3://www.huaraz-adventures.com/ --delete --profile personal --region us-east-1",
    "invalidate:prod": "aws cloudfront create-invalidation --profile personal --distribution-id=E263XA5XPDB0JB --paths /index.html",
    "deploy:prod": "yarn build:prod && yarn push:prod && yarn invalidate:prod",
    "test": "jest --coverage --config jestconfig.json",
    "test:lint": "eslint ./src --fix --ext .js --ext .jsx --cache",
    "test:stylelint": "stylelint --fix 'src/**/*.css' 'src/**/*.scss'"
  },
  "author": "Aldo Funes",
  "license": "ISC",
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.2",
    "@fortawesome/fontawesome-free-brands": "^5.0.4",
