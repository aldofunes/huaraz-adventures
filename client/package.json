    "@fortawesome/fontawesome-free-solid": "^5.0.4",
    "@fortawesome/react-fontawesome": "^0.0.17",
    "apollo-cache-inmemory": "^1.1.5",
    "apollo-client": "^2.2.0",
    "apollo-link": "^1.0.7",
    "apollo-link-context": "^1.0.3",
    "apollo-link-error": "^1.0.3",
    "apollo-link-http": "^1.3.2",
    "aws-sdk": "^2.184.0",
    "babel-core": "^6.26.0",
    "date-fns": "^1.29.0",
    "draft-convert": "^2.0.1",
    "draft-js": "^0.10.4",
    "graphql": "^0.11.7",
    "lodash.debounce": "^4.0.8",
    "node-polyglot": "^2.2.2",
